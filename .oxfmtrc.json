{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "semi": true,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "ignorePatterns": [
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock",
    "node_modules",
    "dist",
    ".tanstack",
    ".netlify"
  ]
}
